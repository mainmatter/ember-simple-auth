    "ember-source-channel-url": "^1.1.0",
    "ember-test-selectors": "^4.0.0",
    "ember-try": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-simplabs": "^0.4.0",
    "eslint-plugin-ember": "^8.1.1",
    "eslint-plugin-mocha": "^6.1.1",
    "eslint-plugin-node": "^11.0.0",
    "express": "^4.17.1",
    "fastboot-app-server": "^2.0.0",
