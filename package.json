    }
  },
  "fastbootDependencies": [
    "node-fetch"
  ],
  "greenkeeper": {
    "ignore": [
      "eslint-plugin-ember",
      "eslint-plugin-mocha"
    ]
  },
  "resolutions": {
    "@babel/plugin-transform-typescript": "~7.4.5",
    "cssstyle": "~1.2.2",
    "jsdom": "^11.10.0"
  },
