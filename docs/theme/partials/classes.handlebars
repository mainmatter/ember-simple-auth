<div class="row">
  <div class="col-sm-12">
    <div class="row">
      <div class="col-md-12">
        <div class="page-header">
          <h1 class="class">{{moduleName}}</h1>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-9">
        <pre><code>import {{moduleName}} from '{{module}}';</code></pre>
        {{{classDescription}}}
      </div>
      <div class="quick-menu col-sm-3">
        {{#if extends}}
          <div class="row">
            <div class="col-sm-12">
              <h4>Extends</h4>
              {{#crossLink extends}}{{/crossLink}}
            </div>
          </div>
        {{/if}}
        {{#if uses}}
          <div class="row">
            <div class="col-sm-12">
              <h4>Uses</h4>
              <ul class="list-unstyled">
                {{#each uses}}
                  <li><a href="{{this}}.html">{{this}}</a></li>
                {{/each}}
              </ul>
            </div>
          </div>
        {{/if}}
        {{#if extension_for}}
          <div class="row">
            <div class="col-sm-12">
              <h4>Extension For</h4>
              <ul class="list-unstyled">
                {{#each extension_for}}
                  <li><a href="{{this}}.html">{{this}}</a></li>
                {{/each}}
              </ul>
            </div>
          </div>
        {{/if}}
        {{#if methods}}
          <div class="row">
            <div class="col-sm-12">
              <h4>Methods</h4>
              <ul class="list-unstyled">
                {{#methods}}
                  <li><a href="#methods_{{name}}">{{name}}</a></li>
                {{/methods}}
              </ul>
            </div>
          </div>
        {{/if}}
        {{#if properties}}
          <div class="row">
            <div class="col-sm-12">
              <h4>Properties</h4>
              <ul class="list-unstyled">
                {{#properties}}
                  <li><a href="#props_{{name}}">{{name}}</a></li>
                {{/properties}}
              </ul>
            </div>
          </div>
        {{/if}}
        {{#if attrs}}
          <div class="row">
            <div class="col-sm-12">
              <h4>Attributes</h4>
              <ul class="list-unstyled">
                {{#attrs}}
                  <li><a href="#attrs_{{name}}">{{name}}</a></li>
                {{/attrs}}
              </ul>
            </div>
          </div>
        {{/if}}
        {{#if events}}
          <div class="row">
            <div class="col-sm-12">
              <h4>Events</h4>
              <ul class="list-unstyled">
                {{#events}}
                  <li><a href="#events_{{name}}">{{name}}</a></li>
                {{/events}}
              </ul>
            </div>
          </div>
        {{/if}}
      </div>
    </div>
    {{#if methods}}
      <div class="row">
        <div class="col-sm-12">
          <h3 class="section">Methods</h3>
        </div>
      </div>
      {{#methods}}
        <div class="row">
          <div class="col-sm-12">
            {{>method}}
          </div>
        </div>
      {{/methods}}
    {{/if}}
  </div>
</div>

<hr/>


<span class="foundat">Class defined in: {{>source_link file=file line=line}}</span>
<div class="intro">{{{classDescription}}}</div>

{{#if is_constructor}}
    {{#is_constructor}}
    {{>method}}
    {{/is_constructor}}
{{/if}}

<div id="classdocs">
    <ul>
        {{#if methods}}
        <li><a href="#methods">Methods</a></li>
        {{/if}}
        {{#if properties}}
        <li><a href="#props">Properties</a></li>
        {{/if}}
        {{#if attrs}}
        <li><a href="#attrs">Attributes</a></li>
        {{/if}}
        {{#if events}}
        <li><a href="#events">Events</a></li>
        {{/if}}
    </ul>
    <div>
        {{#if methods}}
            <div id="methods">
            {{#methods}}
                {{>method}}
            {{/methods}}
            </div>
        {{/if}}
        {{#if properties}}
            <div id="props">
            {{#properties}}
                {{>props}}
            {{/properties}}
            </div>
        {{/if}}
        {{#if attrs}}
            <div id="attrs">
            {{#attrs}}
                {{>attrs}}
            {{/attrs}}
            </div>
        {{/if}}
        {{#if events}}
            <div id="events">
            {{#events}}
                {{>events}}
            {{/events}}
            </div>
        {{/if}}
    </div>
</div>
