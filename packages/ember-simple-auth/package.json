{
  "name": "ember-simple-auth",
  "version": "4.2.2",
  "description": "A lightweight library for implementing authentication/authorization with Ember.js applications.",
  "repository": "https://github.com/mainmatter/ember-simple-auth",
  "license": "MIT",
  "author": "Mainmatter GmbH",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "addon-main.cjs",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "start": "concurrently 'npm:watch:*'",
    "build": "concurrently 'npm:build:*'",
    "build:js": "rollup -c ./rollup.config.mjs",
    "build:docs": "cp ../../README.md ./README.md",
    "watch:js": "rollup -c --watch --no-watch.clearScreen",
    "lint": "concurrently 'npm:lint:js'",
    "lint:fix": "concurrently 'npm:lint:js:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@embroider/addon-shim": "1.8.3",
    "base-64": "^1.0.0",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^3.0.0",
    "broccoli-merge-trees": "^4.0.0",
    "ember-cli-is-package-missing": "^1.0.0",
    "ember-cookies": "^0.5.0",
    "silent-error": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-syntax-decorators": "7.19.0",
    "@babel/plugin-proposal-decorators": "7.20.2",
    "@embroider/addon-dev": "2.0.0",
    "@nullvoxpopuli/eslint-configs": "2.3.5",
    "concurrently": "7.5.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-decorator-position": "5.0.1",
    "eslint-plugin-ember": "11.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "rollup": "3.3.0",
    "babel-eslint": "10.1.0",
    "eslint": "^7.0.0",
    "@rollup/plugin-babel": "6.0.2"
  },
  "peerDependencies": {
    "ember-source": ">= 3.12.0",
    "ember-fetch": "^8.0.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "./addon-main.cjs",
    "app-js": {
      "./initializers/setup-session-restoration.js": "./dist/_app_/initializers/setup-session-restoration.js",
      "./initializers/setup-session.js": "./dist/_app_/initializers/setup-session.js",
      "./services/session.js": "./dist/_app_/services/session.js"
    }
  },
  "engines": {
    "node": ">= 12"
  }
}
